<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

<!--  원래 클래스명은 패키지까지 포함하기때문에
너무 길어서 짧은 별명을 만들어서 접근하기 위해
기술한다  -->
	<typeAliases>
    	
    <typeAlias alias="CommentDto" type="com.semi.flix.comment.CommentDto"/>	
   <typeAlias alias="DramaDto" type="com.semi.flix.drama.DramaDto"/>
   <typeAlias alias="MemberDto" type="com.semi.flix.member.MemberDto"/>	
   <typeAlias alias="MovieDto" type="com.semi.flix.movie.MovieDto"/>
   <typeAlias alias="SearchDto" type="com.semi.flix.search.SearchDto"/>

    	
	</typeAliases>

	<mappers>
	  
		<mapper resource=	"/com/semi/flix/mapper/Member.xml"/>
		<mapper resource=	"/com/semi/flix/mapper/Comment.xml"/>
		<mapper resource=	"/com/semi/flix/mapper/Drama.xml"/>
		<mapper resource=	"/com/semi/flix/mapper/Movie.xml"/>
		<mapper resource=	"/com/semi/flix/mapper/Search.xml"/>
		
	</mappers>
	
</configuration>